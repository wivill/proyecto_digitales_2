T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x278b7b0_0, 0, 1;
T_1.0 ;
    %delay 10, 0;
    %load/vec4 v0x278b7b0_0;
    %inv;
    %store/vec4 v0x278b7b0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x2727d90;
T_2 ;
